<?xml version='1.0' encoding='utf-8'?>
<widget id="com.leandrodavid.whatyousee" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>WhatYouSee</name>
    <description>
        An app that allows user to take a picture and hear the app say what is in the picture. The app will analyze the image and speak what is on it.
    </description>
    <author email="leandrodvd@gmail.com" href="https://github.com/leandrodvd">
        Leandro David
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
